{
  "name": "telemetry_as",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/node/index.js",
      "types": "./dist/types/node/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/types/react/index.d.ts"
    },
    "./react/collectors": {
      "import": "./dist/react/collectors/index.js",
      "types": "./dist/types/react/collectors/index.d.ts"
    },
    "./react/publishers": {
      "import": "./dist/react/publishers/index.js",
      "types": "./dist/types/react/publishers/index.d.ts"
    },
    "./react/schedulers": {
      "import": "./dist/react/schedulers/index.js",
      "types": "./dist/types/react/schedulers/index.d.ts"
    }
  },
  "scripts": {
    "start": "npx tsx ./src/development_index.ts",
    "build": "rd /s /q \"./dist\" && node ./builder.mjs && npx tsc -p tsconfig.declaration.json"
  },
  "author": "Kürşat KORKMAZ",
  "license": "ISC",
  "description": "Node tabanlı tüm projelerde analiz metriklerini toplamanıza yardımcı olan bir kütüphane.",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "esbuild": "0.25.0",
    "esbuild-node-externals": "^1.18.0",
    "jest": "^29.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "prom-client": "^15.1.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4"
  },
  "resolutions": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4"
  }
}
